{
  "name": "@banmao/demo",
  "version": "0.3.1",
  "description": "Datav for vue3+vite",
  "packageManager": "pnpm@9.1.1",
  "author": "banmao",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "datav",
    "data-view",
    "data-view vue3"
  ],
  "homepage": "https://github.com/vaemusic/demo",
  "main": "./dist/demo.es.js",
  "module": "./dist/demo.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/demo.es.js",
      "import": "./dist/demo.es.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:lib": "vue-tsc --noEmit && vite build --config ../../build/button.lib.config.ts",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@jiaminghi/c-render": "^0.4.3",
    "@jiaminghi/charts": "^0.2.18",
    "@jiaminghi/color": "^1.1.3",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.7.2"
  },
  "devDependencies": {
    "@jiaminghi/fs": "^0.1.1",
    "vite-plugin-dts": "^3.7.1",
    "vite-plugin-libcss": "^1.1.1"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaemusic/demo.git"
  },
  "bugs": {
    "url": "https://github.com/vaemusic/demo/issues"
  }
