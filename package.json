{
  "name": "datav-vue3-monorepo",
  "version": "0.1.7",
  "description": "Datav for vue3+vite",
  "packageManager": "pnpm@9.1.1",
  "license": "MIT",
  "private": "true",
  "homepage": "https://github.com/vaemusic/datav-vue3",
  "type": "module",
  "scripts": {
    "dev": "pnpm run --filter @banmao/docs docs:dev",
    "dev:docs": "pnpm run --filter @banmao/docs dev",
    "build:docs": "pnpm run build:lib && pnpm run build",
    "build": "pnpm run --filter @banmao/docs build",
    "build:lib": "pnpm run --filter @banmao/datav-vue3 build:lib",
    "gen": "node ./script/genNewComp/index.js",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "changeset": "changeset"
  },
  "dependencies": {
    "vue": "^3.4.14"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.43.1",
    "@changesets/cli": "^2.27.2",
    "@inquirer/prompts": "^3.3.0",
    "@types/node": "^18.18.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "eslint": "^8.56.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "jsdom": "^23.2.0",
    "pnpm": "^8.14.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0",
    "vue-tsc": "^1.8.27"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaemusic/datav-vue3.git"
  },
  "bugs": {
    "url": "https://github.com/vaemusic/datav-vue3/issues"
  }
}