{
  "name": "banmao-monorepo",
  "type": "module",
  "version": "0.1.7",
  "private": "true",
  "packageManager": "pnpm@9.1.1",
  "description": "Datav for vue3+vite",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banmaoStudio/procomponent.git"
  },
  "bugs": {
    "url": "https://github.com/banmaoStudio/procomponent/issues"
  },
  "scripts": {
    "build:lib": "pnpm run --filter @banmao/procomponent build:lib",
    "gen": "node ./script/genNewComp/index.js",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "changeset": "changeset",
    "docs:dev": "vitepress dev docs",
    "docs:build": "pnpm run build:lib && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@banmao/draw": "workspace:*",
    "@banmao/procomponent": "workspace:*",
    "naive-ui": "^2.38.2",
    "vue": "^3.4.27",
    "vue-draggable-next": "^2.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.43.1",
    "@changesets/cli": "^2.27.2",
    "@css-render/vue3-ssr": "^0.15.14",
    "@inquirer/prompts": "^3.3.0",
    "@types/node": "^18.18.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "eslint": "^8.56.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "jsdom": "^23.2.0",
    "pnpm": "^8.14.1",
    "sass": "^1.77.6",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.0.11",
    "vitepress": "^1.2.2",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.26"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
