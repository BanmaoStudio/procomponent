{
  "name": "datav-vue3-monorepo",
  "version": "0.1.7",
  "description": "Datav for vue3+vite",
  "packageManager": "pnpm@6.32.3",
  "license": "MIT",
  "private": "true",
  "homepage": "https://github.com/vaemusic/datav-vue3",
  "scripts": {
    "dev": "pnpm run --filter @kjgl77/docs dev",
    "build:docs": "pnpm run --filter @kjgl77/docs build:doc",
    "build:lib": "pnpm run --filter @kjgl77/datav-vue3 build:lib",
    "gen": "node ./script/genNewComp/index.js",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "unplugin-auto-import": "^0.7.0",
    "@antfu/eslint-config": "^0.20.2",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.1",
    "eslint": "^8.13.0",
    "inquirer": "^8.2.0",
    "fs-extra": "^10.0.0",
    "jsdom": "^19.0.0",
    "pnpm": "^6.32.6",
    "typescript": "^4.6.3",
    "unocss": "^0.31.0",
    "vite": "^2.9.1",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.2"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaemusic/datav-vue3.git"
  },
  "bugs": {
    "url": "https://github.com/vaemusic/datav-vue3/issues"
  }
}
